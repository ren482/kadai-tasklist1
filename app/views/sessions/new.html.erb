<h1>ログイン画面</h1>

<%= form_with(url: login_path, scope: :session, local: true) do |f| %>


 <%= f.label :email, "Email" %>
 <%= f.email_field :email %>
 
 <%= f.label :password, "Password" %>
 <%= f.password_field :password  %>
 
 <%= f.submit "ログイン" %>
 <%= link_to "Sign up", signup_path %>
<% end %>
